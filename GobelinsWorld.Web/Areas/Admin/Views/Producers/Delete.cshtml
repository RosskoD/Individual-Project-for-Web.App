@model int

@{
    ViewData["Title"] = "Delete Producer";
}
<h2>Are you sure you want to delete this producer?</h2>
<a asp-area="Admin" asp-controller="Producers" asp-action="Destroy" asp-route-id="@Model" class="btn btn-danger">Yes</a>
<a asp-area="Admin" asp-controller="Producers" asp-action="All" class="btn btn-default">Cancel</a>

